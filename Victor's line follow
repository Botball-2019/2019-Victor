void line(){
    while (1){
        if(analog(0) >= 3800) {
            motor(1, -50);
            motor(0, -30);
            msleep(5);
        }
        else if(analog(0) > 3000 && analog(0) < 3800){
            motor(1, -50);
            motor(0, -30);
            msleep(5);
        }
       else {
           motor(1, -45);
           motor(0, -50);
           msleep(5);
       }
    }
 int get_servo_position(int port);             
}

