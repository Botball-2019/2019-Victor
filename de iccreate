#include <kipr/botball.h>

int main()
{
     while(analog(5)< 300){ 
        printf(".");
    }
    shut_down_in(119);
    create_connect();
    
    get_create_battery_capacity();
   
    create_drive_direct(250,250);
    msleep(1000);
    
    create_drive_direct(-250,-250);
    msleep(500);
    
    
    create_drive_direct(200,-200);
    msleep(900);
    
    create_drive_direct(250,250);
    msleep(3300);
    
    create_drive_direct(-200,200);
    msleep(900);
   
       
    create_drive_direct(-100,-100);
    msleep(2300);
       
    create_drive_direct(200,200);
    msleep(2700);
    
     create_drive_direct(-200,200);
     msleep(900);
     
    create_drive_direct(-100,-100);
     msleep(3300);
    
    create_drive_direct(0,0);
    msleep(900);
    
    motor(0,-100);
    msleep(600);
    
    ao();
  
 
    
    
    
    create_drive_direct(200,200);
    msleep(8200);
    
    create_drive_direct(0,0);
    msleep(0);
    
    
    create_drive_direct(200,-200);
    msleep(450);
    
    create_drive_direct(0,0);
    msleep(450);
    
    create_drive_direct(200,200);
    msleep(850);
    
    create_drive_direct(-200,-200);
    msleep(100);
    
    motor(0,100);
    msleep(600);
    
    ao();
    
    create_drive_direct(0,0);
    msleep(450);
    
    create_drive_direct(100,100);
    msleep(900);
    
    
    
    create_drive_direct(-200,0);
    msleep(900);
    
    create_drive_direct(-200,-200);
    msleep(6200);
    
    create_drive_direct(200,-200);
    msleep(900);
    
    create_drive_direct(-200,-200);
    msleep(350);
    
    create_drive_direct(200,-200);
    msleep(950);
    
    create_drive_direct(-200,-200);
    msleep(7250);
    
    create_drive_direct(-200,-250);
    msleep(150);
    
    create_drive_direct(500,500);
    msleep(1000);
    
    create_drive_direct(200,-200);
    msleep(900);
    
    create_drive_direct(200,200);
    msleep(1850);
    
    create_drive_direct(200,-200);
    msleep(2100);

    create_drive_direct(-200,200);
    msleep(1800);
    
    create_drive_direct(200,-200);
    msleep(1800);

    create_drive_direct(-200,200);
    msleep(1800);
    
    return 0;
}


